        "@typescript-eslint/eslint-plugin": "7.15.0",
        "@typescript-eslint/parser": "7.15.0",
        "brotli-compress": "^1.3.3",
        "builtin-modules": "3.3.0",
        "esbuild": "0.23.0",
        "eslint": "9.10.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "moment": "2.30.1",
        "obsidian": "1.6.6",
