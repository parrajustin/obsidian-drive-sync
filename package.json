{
    "name": "obsidian-drive-sync",
    "version": "1.4.1",
    "description": "Syncing of obsidian files using Google drive",
    "main": "main.js",
    "type": "module",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "deploy": "firebase deploy",
        "prebuild": "eslint . --ignore-pattern \"**/*.js\"",
        "build": "node esbuild.config.mjs production",
        "version": "node version-bump.mjs",
        "githubaction": "node version-github-action.mjs",
        "test": "jest",
        "test-dev": "jest --silent=false --verbose=true",
        "lint": "eslint . --ignore-pattern \"**/*.js\"",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "obsidian",
        "obsidian-md",
        "obsidian-plugin",
        "obsidian-md-plugin"
    ],
    "author": "Justin Parra",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "9.10.0",
        "@jest/globals": "29.7.0",
        "@types/gsi": "^0.0.4",
        "@types/node": "22.5.5",
        "@typescript-eslint/eslint-plugin": "8.10.0",
        "@typescript-eslint/parser": "8.10.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.23.0",
        "eslint": "9.10.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "moment": "2.30.1",
        "obsidian": "1.7.2",
        "pre-commit": "1.2.2",
        "prettier": "3.3.2",
        "ts-jest": "29.2.3",
        "ts-node": "10.9.2",
        "tslib": "2.7.0",
        "typescript": "5.6.2",
        "typescript-eslint": "8.10.0"
    },
    "pre-commit": [
        "type-check",
        "lint",
        "test",
        "buid"
    ],
    "dependencies": {
        "firebase": "10.13.2"
    }
}
